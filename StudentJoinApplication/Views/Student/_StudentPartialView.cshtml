@model List<DataAndTranscationsModel>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<table class="table table-bordered">
    <thead>
        <tr>
            <th scope="row"> ID</th>
            <th> Name</th>
            <th> Class</th>
            <th> Age</th>
            <th> Date</th>
            <th> Fees</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <th scope="row">@student.ID</th>
                <td>@student.Name</td>
                <td>@student.Class</td>
                <td>@student.Age</td>
                <td>@student.Date</td>
                <td>@student.Fees</td>

            </tr>
        }

    </tbody>
</table>
